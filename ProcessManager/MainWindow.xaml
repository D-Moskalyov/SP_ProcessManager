<Window x:Class="ProcessManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="616" Width="367">
    <Grid>
        <ListBox Name = "listBox" HorizontalAlignment="Left" Height="534" Margin="10,10,0,0" VerticalAlignment="Top" Width="259" MouseDoubleClick="listBox_MouseDoubleClick"/>
        <Button Name = "kill" Content="Kill" HorizontalAlignment="Left" Margin="274,102,0,0" VerticalAlignment="Top" Width="75" Click="kill_Click"/>
        <Button Name = "copy" Content="Copy" HorizontalAlignment="Left" Margin="274,77,0,0" VerticalAlignment="Top" Width="75" Click="copy_Click"/>
        <Button Name = "browse" Content="Browse" HorizontalAlignment="Left" Margin="274,10,0,0" VerticalAlignment="Top" Width="75" Click="browse_Click"/>
        <Button Name = "exe" Content="*.exe" HorizontalAlignment="Left" Margin="274,171,0,0" VerticalAlignment="Top" Width="75" Click="exe_Click"/>
        <Label Name = "labal" Content=".exe scan" HorizontalAlignment="Left" Margin="10,549,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.368,-0.269"/>
        <ProgressBar Name = "progressBar" HorizontalAlignment="Left" Height="17" Margin="72,558,0,0" VerticalAlignment="Top" Width="197"/>

    </Grid>
</Window>
